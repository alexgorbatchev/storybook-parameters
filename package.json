{
  "name": "@alexgorbatchev/storybook-parameters",
  "version": "3.0.1",
  "description": "TypeScript helpers for strongly typed Storybook parameters",
  "keywords": [
    "storybook",
    "parameters",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexgorbatchev/storybook-parameters"
  },
  "authors": [
    "Alex Gorbatchev <alex.gorbatchev@gmail.com>"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "node": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.mts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js",
      "node": "./dist/react.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.png",
    "!src/**/*"
  ],
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "@storybook/react": "^8.1.5",
    "@storybook/types": "^8.1.5",
    "@swc/core": "^1.5.27",
    "@types/react": "^18.3.3",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "storybook": "^8.1.5",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
